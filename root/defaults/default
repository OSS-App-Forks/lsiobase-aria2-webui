server {
  listen 80;
  server_name _;
  return 301 https://$server_name$request_uri;
}

server {
  listen 443 ssl;
  server_name _;

  ssl_certificate /config/keys/cert.crt;
  ssl_certificate_key /config/keys/cert.key;

  location / {
    root /data/aria2c-webui;
  }
    
  location /jsonrpc {
    proxy_pass http://127.0.0.1:6800;
  }

}
